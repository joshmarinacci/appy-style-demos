{
  "name": "appy-style-demos",
  "version": "0.0.0",
  "description": "Demos for the appy-style css framework",
  "main": "index.js",
  "scripts": {
    "build-js": "browserify -d -t reactify browser/ui.jsx -o browser/bundle.js",
    "browser": "node browser/start.js",
    "build-musicplayer": "browserify -d -t reactify musicplayer/ui.jsx -o musicplayer/bundle.js",
    "watch-musicplayer": "watchify   -d -t reactify musicplayer/ui.jsx -o musicplayer/bundle.js",
    "musicplayer": "node musicplayer/start.js",
    "watch-todolist": "watchify -d -t reactify todolist/ui.jsx -o todolist/bundle.js",
    "postinstall": "node_modules/bower/bin/bower install -F"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshmarinacci/appy-style-demos.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joshmarinacci/appy-style-demos/issues"
  },
  "homepage": "https://github.com/joshmarinacci/appy-style-demos",
  "dependencies": {
    "async-walker": "0.0.4",
    "bower": "^1.4.1",
    "fixed-data-table": "^0.1.2",
    "lame": "^1.2.0",
    "moment": "^2.9.0",
    "musicmetadata": "^1.0.1",
    "nedb": "^1.1.2",
    "node-dir": "^0.1.6",
    "node-thrust": "^0.7.6-rc.1",
    "object-assign": "^2.0.0",
    "q": "^1.2.0",
    "react": "^0.13.1",
    "reactify": "^1.1.0",
    "speaker": "^0.2.4"
  }
}
